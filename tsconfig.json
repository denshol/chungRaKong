{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types"],
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"],
      "@react-navigation/*": ["node_modules/@react-navigation/*"]
    },
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
